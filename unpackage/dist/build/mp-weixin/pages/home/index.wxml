<view class="content"><view class="banner"><swiper class="swiper-box" autoplay="{{autoplay}}" indicator-dots="{{indicatorDots}}" interval="{{interval}}" circular="{{circular}}"><block wx:for="{{banner}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image class="banner-image" src="{{item}}"></image></swiper-item></block></swiper><view data-event-opts="{{[['tap',[['scanCode',['$event']]]]]}}" class="scan" bindtap="__e"><uni-icon vue-id="1" type="scan" bind:__l="__l"></uni-icon></view></view><view class="list"><block wx:for="{{listObj}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['go',['$0'],[[['listObj','',index,'url']]]]]]]}}" class="list-cell" bindtap="__e"><view class="list-text">{{item.liText+''}}<view hidden="{{!(item.hint)}}" class="list-hint">{{item.hint}}</view></view><view class="list-en">{{item.liEn}}</view><view class="list-right"><image class="list-img" src="{{item.imgurl}}" mode="aspectFit"></image></view></view></block></view></view>